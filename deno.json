{
  "compilerOptions": {
    "exactOptionalPropertyTypes": true,
    "noImplicitOverride": true,
    "noUncheckedIndexedAccess": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true
  },
  "fmt": { "semiColons": false, "singleQuote": true },
  "imports": { "std/": "https://deno.land/std@0.192.0/" },
  "lint": { "rules": { "exclude": ["no-inferrable-types"] } },
  "lock": false,
  "name": "solitaire",
  "tasks": {
    "test": "deno check mod.ts && deno test --allow-read=. && deno lint && deno fmt --check"
  },
  "version": "0.0.9"
}
